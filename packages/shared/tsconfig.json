{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2022",
    "esModuleInterop": true,
    "strict": true,
    "noImplicitOverride": true,
    "module": "nodenext",
    "sourceMap": true,
    "declaration": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "lib": [
      "ES2022"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  }
}